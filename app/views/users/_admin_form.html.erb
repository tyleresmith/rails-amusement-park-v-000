<%= form_for user, as: :admin, html: { id: 'user_admin'} do |f| %>
  <%= f.label :name %>:
  <%= f.text_field :name %><br />

  <%= f.label :nausea %>:
  <%= f.text_field :nausea %><br />
  <%= f.label :happiness %>:
  <%= f.text_field :happiness %><br />
  <%= f.label :tickets %>:
  <%= f.text_field :tickets %><br />
  <%= f.label :height %>:
  <%= f.text_field :height %><br />

  <%= f.submit "Create Admin"%>
<% end %>
